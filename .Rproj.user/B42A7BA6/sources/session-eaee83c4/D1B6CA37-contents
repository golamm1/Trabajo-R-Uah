#6.5.1 Etiquetar los Rangos de Sueldos dentro de la variable de Sueldos
Casen_Filtrado$y1Agrupado <- factor(Casen_Filtrado$y1Agrupado,
                                      levels = c(1, 2, 3, 4, 5),
                                      labels = c("[0-500.000]", "[500.001-1.000.000]", "[1.000.001-5.000.000]", "[5.000.001-10.000.000]", "[10.000.001-15.000.000]"))

# 7 creacion de tablas 

# 7.1 Tabla de Frecuencia Absoluta

table(Casen_Filtrado$sexo)
fa <- table(Casen_Filtrado$sexo)

table(Casen_Filtrado$region)
fe <- table(Casen_Filtrado$region)

table(Casen_Filtrado$y1)
fi <- table(Casen_Filtrado$y1)

table(Casen_Filtrado$s26a)
fo <- table(Casen_Filtrado$s26a)

table(Casen_Filtrado$EdadCuali)
fu <- table(Casen_Filtrado$EdadCuali)

table(Casen_Filtrado$s26aAgrupado)
fa2 <- table(Casen_Filtrado$s26aAgrupado)

table(Casen_Filtrado$y1Agrupado)
fi2 <- table(Casen_Filtrado$y1Agrupado)

# 7.2 Proporciones o frecuencia relativa

prop.table(fa)
prop <- prop.table(fa)
prop <- round(prop, 3)

prop.table(fe)
prop2 <- prop.table(fe)
prop2 <- round(prop2, 3)

prop.table(fi)
prop3 <- prop.table(fi)
prop3 <- round(prop3, 3)

prop.table(fo)
prop4 <- prop.table(fo)
prop4 <- round(prop4, 3)

prop.table(fu)
prop5 <- prop.table(fu)
prop5 <- round(prop5, 3)

prop.table(fa2)
prop6 <- prop.table(fa2)
prop6 <- round(prop6, 3)

prop.table(fi2)
prop7 <- prop.table(fi2)
prop7 <- round(prop7, 3)


# 7.3 Porcentajes 

prop*100
porcentaje <- prop*100

prop2*100
porcentaje2 <- prop2*100

prop3*100
porcentaje3 <- prop3*100

prop4*100
porcentaje4 <- prop4*100

prop5*100
porcentaje5 <- prop5*100

prop6*100
porcentaje6 <- prop6*100

prop7*100
porcentaje7 <- prop7*100


# 7.4. Crear la tabla + tabla de fila de totales

tabla_Sexo <- data.frame("Categoría" = names(fa),
                    "Frecuencia" = as.numeric(fa),
                    "Proporción" = as.numeric(prop),
                    "Porcentaje" = as.numeric(porcentaje))      

tabla_fila_total_Sexo <- data.frame("Categoría" = "Total",
                                "Frecuencia" = sum(fa),
                                "Proporción" = sum(prop),
                                "Porcentaje" = sum(porcentaje))


tabla_Regiones <- data.frame("Categoría" = names(fe),
                       "Frecuencia" = as.numeric(fe),
                       "Proporción" = as.numeric(prop2),
                       "Porcentaje" = as.numeric(porcentaje2))    

tabla_fila_total_Regiones <- data.frame("Categoría" = "Total",
                                   "Frecuencia" = sum(fe),
                                   "Proporción" = sum(prop2),
                                   "Porcentaje" = sum(porcentaje2))

tabla_Sueldos <- data.frame("Categoría" = names(fi),
                       "Frecuencia" = as.numeric(fi),
                       "Proporción" = as.numeric(prop3),
                       "Porcentaje" = as.numeric(porcentaje3))    

tabla_fila_total_Sueldos <- data.frame("Categoría" = "Total",
                                   "Frecuencia" = sum(fi),
                                   "Proporción" = sum(prop3),
                                   "Porcentaje" = sum(porcentaje3))

tabla_Visitas_Medico <- data.frame("Categoría" = names(fo),
                       "Frecuencia" = as.numeric(fo),
                       "Proporción" = as.numeric(prop4),
                       "Porcentaje" = as.numeric(porcentaje4))    

tabla_fila_total_Visitas_Medico <- data.frame("Categoría" = "Total",
                                   "Frecuencia" = sum(fo),
                                   "Proporción" = sum(prop4),
                                   "Porcentaje" = sum(porcentaje4))

tabla_edad <- data.frame("Categoría" = names(fu),
                       "Frecuencia" = as.numeric(fu),
                       "Proporción" = as.numeric(prop5),
                       "Porcentaje" = as.numeric(porcentaje5))    

tabla_fila_total_edad <- data.frame("Categoría" = "Total",
                                   "Frecuencia" = sum(fu),
                                   "Proporción" = sum(prop5),
                                   "Porcentaje" = sum(porcentaje5))

tabla_Visitas_Medico_Agrupados <- data.frame("Categoría" = names(fa2),
                                             "Frecuencia" = as.numeric(fa2),
                                             "Proporción" = as.numeric(prop6),
                                             "Porcentaje" = as.numeric(porcentaje6))    

tabla_fila_total_Medico_Agrupados <- data.frame("Categoría" = "Total",
                                                "Frecuencia" = sum(fa2),
                                                "Proporción" = sum(prop6),
                                                "Porcentaje" = sum(porcentaje6))

tabla_Sueldos_Agrupados <- data.frame("Categoría" = names(fi2),
                                      "Frecuencia" = as.numeric(fi2),
                                      "Proporción" = as.numeric(prop7),
                                      "Porcentaje" = as.numeric(porcentaje7))    

tabla_fila_total_Sueldos_Agrupados <- data.frame("Categoría" = "Total",
                                      "Frecuencia" = sum(fi2),
                                      "Proporción" = sum(prop7),
                                      "Porcentaje" = sum(porcentaje7))

# 7.5 Tabla Final 

Tabla_final_Sexo <- rbind (tabla_Sexo, tabla_fila_total_Sexo)

Tabla_final_Regiones <- rbind (tabla_Regiones, tabla_fila_total_Regiones)

Tabla_final_Sueldos <- rbind (tabla_Sueldos, tabla_fila_total_Sueldos)

Tabla_final_Visitas_Medico <- rbind (tabla_Visitas_Medico, tabla_fila_total_Visitas_Medico)

Tabla_final_edad <- rbind(tabla_edad, tabla_fila_total_edad)

Tabla_final_Visitas_Medico_Agrupados <- rbind(tabla_Visitas_Medico_Agrupados, tabla_fila_total_Medico_Agrupados)

Tabla_final_Sueldos_Agrupados <- rbind(tabla_Sueldos_Agrupados, tabla_fila_total_Sueldos_Agrupados)


# 7.6 Tabla de Formato

Casen_Filtrado%>%
  group_by(sexo) %>%
  summarise(Frecuencia = n()) %>%
  mutate(Proporción = round(Frecuencia / sum(Frecuencia), 3)) %>%
  mutate(Porcentaje = round(Frecuencia / sum(Frecuencia), 3)*100)


Casen_Filtrado%>%
  group_by(region) %>%
  summarise(Frecuencia = n()) %>%
  mutate(Proporción = round(Frecuencia / sum(Frecuencia), 3)) %>%
  mutate(Porcentaje = round(Frecuencia / sum(Frecuencia), 3)*100)

Casen_Filtrado%>%
  group_by(y1) %>%
  summarise(Frecuencia = n()) %>%
  mutate(Proporción = round(Frecuencia / sum(Frecuencia), 3)) %>%
  mutate(Porcentaje = round(Frecuencia / sum(Frecuencia), 3)*100)

Casen_Filtrado%>%
  group_by(s26a) %>%
  summarise(Frecuencia = n()) %>%
  mutate(Proporción = round(Frecuencia / sum(Frecuencia), 3)) %>%
  mutate(Porcentaje = round(Frecuencia / sum(Frecuencia), 3)*100)

Casen_Filtrado%>%
  group_by(EdadCuali) %>%
  summarise(Frecuencia = n()) %>%
  mutate(Proporción = round(Frecuencia / sum(Frecuencia), 3)) %>%
  mutate(Porcentaje = round(Frecuencia / sum(Frecuencia), 3)*100)

Casen_Filtrado%>%
  group_by(s26aAgrupado) %>%
  summarise(Frecuencia = n()) %>%
  mutate(Proporción = round(Frecuencia / sum(Frecuencia), 3)) %>%
  mutate(Porcentaje = round(Frecuencia / sum(Frecuencia), 3)*100)


Casen_Filtrado%>%
  group_by(y1Agrupado) %>%
  summarise(Frecuencia = n()) %>%
  mutate(Proporción = round(Frecuencia / sum(Frecuencia), 3)) %>%
  mutate(Porcentaje = round(Frecuencia / sum(Frecuencia), 3)*100)


# 7.7 Tablas de contingencias

tabla_contingencia_Sexo_Sueldos <- tab_xtab(var.row = Casen_Filtrado$sexo, var.col = Casen_Filtrado$y1Agrupado,
                               title = "Tabla 1. Sueldos segun Sexo", var.labels = c("Sexo", "Sueldo"),
                               show.row.prc = FALSE, show.cell.prc = FALSE, show.col.prc = TRUE, show.summary = FALSE)

tabla_contingencia_Sexo_Sueldos

                             
tabla_contingencia_Sexo_Visita_Medica <- tab_xtab(var.row = Casen_Filtrado$sexo, var.col = Casen_Filtrado$s26aAgrupado,
                                            title = "Tabla 1. Visitas al medico segun sexo", var.labels = c("Sexo", "visitas al medico"),
                                            show.row.prc = FALSE, show.cell.prc = FALSE, show.col.prc = TRUE, show.summary = FALSE)

tabla_contingencia_Sexo_Visita_Medica



tabla_contingencia_Sueldos_Visitas_Medicas <- tab_xtab(var.row = Casen_Filtrado$y1Agrupado, var.col = Casen_Filtrado$s26aAgrupado,
                                                  title = "Tabla 1. Visitas al medico segun sexo", var.labels = c("Sueldos", "visitas al medico"),
                                                  show.row.prc = FALSE, show.cell.prc = FALSE, show.col.prc = TRUE, show.summary = FALSE)

tabla_contingencia_Sueldos_Visitas_Medicas



tabla_contingencia_Edad_Visitas_Medicas <- tab_xtab(var.row = Casen_Filtrado$EdadCuali, var.col = Casen_Filtrado$s26aAgrupado,
                                                       title = "Tabla 1. Visitas al medico segun grupos etarios", var.labels = c("Edad", "visitas al medico"),
                                                       show.row.prc = FALSE, show.cell.prc = FALSE, show.col.prc = TRUE, show.summary = FALSE)

tabla_contingencia_Edad_Visitas_Medicas


tabla_contingencia_Region_Visitas_Medicas <- tab_xtab(var.row = Casen_Filtrado$region, var.col = Casen_Filtrado$s26aAgrupado,
                                                    title = "Tabla 1. Visitas al medico segun grupos etarios", var.labels = c("Edad", "visitas al medico"),
                                                    show.row.prc = FALSE, show.cell.prc = FALSE, show.col.prc = TRUE, show.summary = FALSE)

tabla_contingencia_Region_Visitas_Medicas



# 8. Establecer los descriptivos de las variables Cuantitativas 

# 8.1 Variable de Visitas al Medico 
Consultas_Salud = descr(Casen_Filtrado$s26a, na.rm = TRUE)


# 8.2 Variable de Sueldos Liquirdos del ultimo mes 
Consultas_sueldos = descr(Casen_Filtrado$y1, na.rm = TRUE)


# 9. Curvas de Densidad de las Variables Cuantitativas 


#9.1 Curva de Densidad de la Variable de visitas Medicas 
plot(density(Casen_Filtrado$s26a,bw = 2 ,na.rm = T))


#9.2 Curva de Densidad de la Variable de Sueldos dentro del Ultimo mes
plot(density(Casen_Filtrado$y1,bw = 2 ,na.rm = T))

#9.3 Curva de Densidad de la Variable edad
plot(density(Casen_Filtrado$edad,bw = 2 ,na.rm = T))


